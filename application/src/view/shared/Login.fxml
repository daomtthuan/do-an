<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="196.0" prefWidth="403.0" xmlns="http://javafx.com/javafx/8">
    <children>
        <VBox alignment="CENTER" prefHeight="208.0" prefWidth="418.0" spacing="10.0">
            <children>
                <Label alignment="CENTER" text="LOGIN">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Label>
                <GridPane hgap="10.0" prefHeight="75.0" prefWidth="456.0" vgap="10.0">
                    <columnConstraints>
                        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="191.0" minWidth="10.0"
                                           prefWidth="100.0"/>
                        <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="432.0" minWidth="10.0"
                                           prefWidth="326.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints valignment="CENTER" vgrow="SOMETIMES"/>
                        <RowConstraints valignment="CENTER" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label alignment="CENTER" text="Account">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                        <Label text="Password" GridPane.rowIndex="1">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="account" GridPane.columnIndex="1">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </TextField>
                        <TextField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </TextField>
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </GridPane>
                <Button alignment="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#login"
                        prefHeight="31.0" prefWidth="108.0" text="Login">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Button>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </children>
</AnchorPane>
