<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane styleClass="customAnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.customer.ControllerOrder">
   <children>
       <VBox spacing="10.0" styleClass="customVBox" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
           <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
           </padding>
          <children>
              <Label styleClass="customTitle" text="ORDER FOOD" VBox.vgrow="ALWAYS" />
            <HBox styleClass="customHBox" VBox.vgrow="ALWAYS">
               <children>
                     <Button fx:id="discountButton" mnemonicParsing="false" prefHeight="70.0" prefWidth="200.0" styleClass="customButton" />
                     <Button fx:id="informationButton" mnemonicParsing="false" prefHeight="70.0" prefWidth="200.0" styleClass="customButton" />
                     <Button fx:id="accountButton" mnemonicParsing="false" prefHeight="70.0" prefWidth="200.0" styleClass="customButton" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
              <SplitPane dividerPositions="0.6577235772357723" styleClass="customSplitPane" VBox.vgrow="ALWAYS">
                 <items>
                  <AnchorPane fx:id="viewFood" />
                  <AnchorPane fx:id="viewBill" />
                 </items>
              </SplitPane>
          </children>
       </VBox>
   </children>
</AnchorPane>
