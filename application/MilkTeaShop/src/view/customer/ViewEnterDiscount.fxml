<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane styleClass="customAnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.customer.ControllerEnterDiscount">
    <children>
        <VBox styleClass="customVBox">
            <children>
                <Label styleClass="customTitle" text="ENTER DISCOUNT" />
                <GridPane styleClass="customGridPane">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="169.0" minWidth="10.0" prefWidth="50.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="277.0" minWidth="10.0" prefWidth="277.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label styleClass="customLable" text="Name" />
                        <TextField fx:id="name" onAction="#submit" prefHeight="30.0" prefWidth="269.0" styleClass="customTextField" GridPane.columnIndex="1" />
                    </children>
                </GridPane>
                <Button mnemonicParsing="false" onAction="#submit" styleClass="customButton" text="Submit" />
            </children>
        </VBox>
    </children>
</AnchorPane>
