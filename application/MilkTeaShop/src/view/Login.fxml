<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="181.0" prefWidth="376.0" xmlns="http://javafx.com/javafx/8">
    <VBox alignment="CENTER" prefHeight="224.0" prefWidth="393.0" spacing="10.0">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <Label alignment="CENTER" text="LOGIN" textAlignment="CENTER" textFill="#2e6681">
            <font>
                <Font name="System Bold" size="24.0"/>
            </font>
        </Label>
        <GridPane hgap="10.0" prefHeight="51.0" prefWidth="356.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="191.0" minWidth="10.0"
                                   prefWidth="51.0"/>
                <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="432.0" minWidth="10.0"
                                   prefWidth="275.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints valignment="CENTER" vgrow="SOMETIMES"/>
                <RowConstraints valignment="CENTER" vgrow="SOMETIMES"/>
            </rowConstraints>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <Label alignment="CENTER" text="Account">
                <font>
                    <Font size="14.0"/>
                </font>
            </Label>
            <Label text="Password" GridPane.rowIndex="1">
                <font>
                    <Font size="14.0"/>
                </font>
            </Label>
            <TextField fx:id="name" prefHeight="25.0" prefWidth="268.0" GridPane.columnIndex="1">
                <font>
                    <Font size="14.0"/>
                </font>
            </TextField>
            <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1">
                <font>
                    <Font size="14.0"/>
                </font>
            </PasswordField>
        </GridPane>
        <Button alignment="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#login"
                prefHeight="31.0" prefWidth="108.0" text="Login">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
    </VBox>
</AnchorPane>
