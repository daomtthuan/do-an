                        </HBox>
                        <DatePicker fx:id="birthday" editable="false" prefHeight="30.0" prefWidth="385.0" styleClass="customDatePicker" GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
