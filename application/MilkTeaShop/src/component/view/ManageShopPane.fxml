<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import java.lang.*?>

<HBox id="managePane" fx:id="manageMenuPane" styleClass="customHBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox>
            <children>
                <Button mnemonicParsing="false" onAction="#managePositionTable" text="Position Table">
                    <styleClass>
                        <String fx:value="customButton" />
                        <String fx:value="manageMenuButton" />
                    </styleClass>
                </Button>
            <Button mnemonicParsing="false" onAction="#manageTable" text="Table">
               <styleClass>
                  <String fx:value="customButton" />
                  <String fx:value="manageMenuButton" />
               </styleClass>
            </Button>
                <Button mnemonicParsing="false" onAction="#manageBill" text="Bill">
                    <styleClass>
                        <String fx:value="customButton" />
                        <String fx:value="manageMenuButton" />
                    </styleClass>
                </Button>
                <Button mnemonicParsing="false" onAction="#manageDiscount" text="Discount">
                    <styleClass>
                        <String fx:value="customButton" />
                        <String fx:value="manageMenuButton" />
                    </styleClass>
                </Button>
            </children>
            <styleClass>
                <String fx:value="customVBox" />
                <String fx:value="manageButtonPane" />
            </styleClass>
        </VBox>
        <VBox fx:id="subManageComponent" styleClass="customVBox" />
    </children>
</HBox>
