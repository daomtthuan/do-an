<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox styleClass="customHBox">
         <children>
            <Button fx:id="insertButton" mnemonicParsing="false" styleClass="customButton" text="Insert" />
            <Button fx:id="updateButton" mnemonicParsing="false" styleClass="customButton" text="Update" />
            <Button fx:id="deleteButton" mnemonicParsing="false" styleClass="customButton" text="Delete" />
         </children>
      </HBox>
      <TableView fx:id="tableView">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <styleClass>
            <String fx:value="customTableView" />
            <String fx:value="tableView" />
         </styleClass></TableView>
   </children>
   <styleClass>
      <String fx:value="customVBox" />
      <String fx:value="managePane" />
   </styleClass>
</VBox>
