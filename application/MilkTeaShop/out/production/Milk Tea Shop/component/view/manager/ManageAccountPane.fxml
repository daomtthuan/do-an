<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox id="managePane" fx:id="manageAccountPane" styleClass="customHBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <Button mnemonicParsing="false" onAction="#manageCustomer" text="Customer">
               <styleClass>
                  <String fx:value="customButton" />
                  <String fx:value="manageAccountButton" />
               </styleClass>
            </Button>
            <Button mnemonicParsing="false" onAction="#manageEmployee" text="Employee">
               <styleClass>
                  <String fx:value="customButton" />
                  <String fx:value="manageAccountButton" />
               </styleClass>
            </Button>
            <Button mnemonicParsing="false" onAction="#manageManager" text="Manager">
               <styleClass>
                  <String fx:value="customButton" />
                  <String fx:value="manageAccountButton" />
               </styleClass>
            </Button>
         </children>
         <styleClass>
            <String fx:value="customVBox" />
            <String fx:value="manageButtonPane" />
         </styleClass>
      </VBox>
      <VBox fx:id="subManageComponent" styleClass="customVBox" />
   </children>
</HBox>
