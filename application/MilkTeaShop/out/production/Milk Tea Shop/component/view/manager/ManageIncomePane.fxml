<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="managePane" fx:id="manageIncomePane" styleClass="customVBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane styleClass="customGridPane">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" />
          <ColumnConstraints hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label styleClass="topTitle" text="Top buy" />
            <Label styleClass="topTitle" text="Top sell" GridPane.columnIndex="1" />
            <Label fx:id="idCustomerLabel" text="Label" GridPane.rowIndex="1" />
            <Label fx:id="nameCustomerLabel" text="Label" GridPane.rowIndex="2" />
            <Label fx:id="totalBuyLabel" text="Label" GridPane.rowIndex="3" />
            <Label fx:id="nameFoodLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="totalQuantityLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="totalSellLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
      <VBox fx:id="chartPane" styleClass="customVBox" />
   </children>
</VBox>
