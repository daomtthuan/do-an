<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" styleClass="customAnchorPane" xmlns="http://javafx.com/javafx/8">
    <children>
        <VBox styleClass="customVBox">
            <children>
                <Label styleClass="customTitle" text="ADD CATEGORY"/>
                <GridPane styleClass="customGridPane">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="158.0" minWidth="10.0" prefWidth="40.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="286.0" minWidth="10.0" prefWidth="286.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label styleClass="customLable" text="Name"/>
                        <TextField styleClass="customField" GridPane.columnIndex="1"/>
                    </children>
                </GridPane>
                <Button mnemonicParsing="false" styleClass="customButton" text="Submit"/>
            </children>
        </VBox>
    </children>
</AnchorPane>
