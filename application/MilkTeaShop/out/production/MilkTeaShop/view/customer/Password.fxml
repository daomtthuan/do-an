<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<HBox xmlns:fx="http://javafx.com/fxml/1" id="wrapper" styleClass="customHBox" xmlns="http://javafx.com/javafx/8"
      fx:controller="controller.customer.Password">
    <children>
        <VBox id="container" styleClass="customVBox">
            <children>
                <Label styleClass="customTitle" text="Register"/>
                <GridPane styleClass="customGridPane">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="185.0" minWidth="10.0" prefWidth="124.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="272.0" minWidth="10.0" prefWidth="226.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label styleClass="customLable" text="Password"/>
                        <Label styleClass="customLable" text="Repeat Password" GridPane.rowIndex="1"/>
                        <PasswordField styleClass="customField" GridPane.columnIndex="1"/>
                        <PasswordField styleClass="customField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                    </children>
                </GridPane>
                <Button mnemonicParsing="false" styleClass="customButton" text="Submit"/>
            </children>
        </VBox>
    </children>
</HBox>
