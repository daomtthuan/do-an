<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" styleClass="customAnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.customer.ControllerPassword">
   <children>
      <VBox prefHeight="300.0" prefWidth="400.0" spacing="10.0" styleClass="customVBox">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Label styleClass="customTitle" text="Register" />
            <GridPane hgap="10.0" styleClass="customGridPane" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="185.0" minWidth="10.0" prefWidth="124.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="272.0" minWidth="10.0" prefWidth="226.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label styleClass="customLable" text="Password" />
                  <Label styleClass="customLable" text="Repeat Password" GridPane.rowIndex="1" />
                  <PasswordField styleClass="customPasswordField" GridPane.columnIndex="1" />
                  <PasswordField styleClass="customPasswordField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </GridPane>
            <Button defaultButton="true" mnemonicParsing="false" prefHeight="33.0" prefWidth="110.0" styleClass="customButton" text="Submit" />
         </children>
      </VBox>
   </children>
</AnchorPane>
