<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="181.0" prefWidth="376.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="controller.customer.ControllerEnterDiscount">
    <VBox alignment="CENTER" prefHeight="181.0" prefWidth="377.0" spacing="10.0">
