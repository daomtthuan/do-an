<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="390.0" prefWidth="452.0" xmlns="http://javafx.com/javafx/8">
    <VBox alignment="CENTER" layoutX="250.0" layoutY="74.0" prefHeight="483.0" prefWidth="600.0" spacing="10.0"
          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0">
        <opaqueInsets>
            <Insets/>
        </opaqueInsets>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <Label fx:id="title" contentDisplay="CENTER" text="Register" textFill="#2e6681">
            <font>
                <Font name="System Bold" size="24.0"/>
            </font>
        </Label>
        <GridPane hgap="10.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="196.0" minWidth="10.0"
                                   prefWidth="71.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="339.0" minWidth="10.0" prefWidth="331.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <Label text="Name">
                <font>
                    <Font size="14.0"/>
                </font>
            </Label>
            <Label text="Gender" GridPane.rowIndex="1">
                <font>
                    <Font size="14.0"/>
                </font>
            </Label>
            <Label text="Birthday" GridPane.rowIndex="2">
                <font>
                    <Font size="14.0"/>
                </font>
            </Label>
            <Label text="Address" GridPane.rowIndex="3">
                <font>
                    <Font size="14.0"/>
                </font>
            </Label>
            <Label text="Email" GridPane.rowIndex="5">
                <font>
                    <Font size="14.0"/>
                </font>
            </Label>
            <Label text="Phone" GridPane.rowIndex="4">
                <font>
                    <Font size="14.0"/>
                </font>
            </Label>
            <TextField fx:id="name" prefHeight="30.0" prefWidth="329.0" GridPane.columnIndex="1">
                <font>
                    <Font size="14.0"/>
                </font>
            </TextField>
            <TextField fx:id="phone" GridPane.columnIndex="1" GridPane.rowIndex="4">
                <font>
                    <Font size="14.0"/>
                </font>
            </TextField>
            <TextField fx:id="email" GridPane.columnIndex="1" GridPane.rowIndex="5">
                <font>
                    <Font size="14.0"/>
                </font>
            </TextField>
            <TextField fx:id="address" prefHeight="25.0" prefWidth="300.0" GridPane.columnIndex="1"
                       GridPane.rowIndex="3">
                <font>
                    <Font size="14.0"/>
                </font>
            </TextField>
            <DatePicker fx:id="birthday" prefHeight="55.0" prefWidth="331.0" GridPane.columnIndex="1"
                        GridPane.rowIndex="2" GridPane.valignment="CENTER"/>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="10.0"
                  GridPane.columnIndex="1" GridPane.rowIndex="1">
                <RadioButton fx:id="male" contentDisplay="CENTER" mnemonicParsing="false" text="Male"
                             textOverrun="CENTER_ELLIPSIS">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </RadioButton>
                <RadioButton fx:id="female" contentDisplay="CENTER" mnemonicParsing="false"
                             text="Female" textOverrun="CENTER_ELLIPSIS">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </RadioButton>
            </HBox>
        </GridPane>
        <Button mnemonicParsing="false" onAction="#submit" text="Submit">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
    </VBox>
</AnchorPane>
