<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="container" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity"
      styleClass="customVBox" xmlns="http://javafx.com/javafx/8">
    <ScrollPane fitToHeight="true" fitToWidth="true" styleClass="customScrollPane">
        <HBox fx:id="categoryPane" styleClass="customHBox"/>
    </ScrollPane>
    <ScrollPane fitToHeight="true" fitToWidth="true" styleClass="customScrollPane">
        <content>
            <FlowPane fx:id="foodPane" styleClass="customFlowPane"/>
        </content>
    </ScrollPane>
</VBox>
